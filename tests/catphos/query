query 'analyze_cycles.csv' {'substrate_id', 'binding_time', 'unbinding_time'}
match b:{ s:S(d[/1]), k:K(d[/1], x{u}) }
and first u:{ s:S(d[/.]) } after b
when time[b] >= 5
return (agent_id{s}, time[b], time[u])


query 'trace.csv'
match e return time[e], rule[e]